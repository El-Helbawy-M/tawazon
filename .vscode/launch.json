    "version": "0.2.0",
    "configurations": [
        {
            "name": "tawazon (Debug)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--dart-define=ENVIRONMENT=development",
                "--dart-define=API_BASE_URL=https://dev-api.tawazon.com"
            ],
            "console": "debugConsole",
        },
        {
            "name": "tawazon (Debug - Mobile)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--dart-define=ENVIRONMENT=development",
                "--dart-define=API_BASE_URL=https://dev-api.tawazon.com"
            ],
            "console": "debugConsole"
        },
        {
            "name": "tawazon (Staging)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--dart-define=ENVIRONMENT=staging",
                "--dart-define=API_BASE_URL=https://staging-api.tawazon.com"
            ],
            "flutterMode": "debug"
        },
        {
            "name": "tawazon (Profile)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "flutterMode": "profile",
            "args": [
                "--dart-define=ENVIRONMENT=production",
                "--dart-define=API_BASE_URL=https://api.tawazon.com"
            ]
        },
        {
            "name": "tawazon (Release)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "flutterMode": "release",
            "args": [
                "--dart-define=ENVIRONMENT=production",
                "--dart-define=API_BASE_URL=https://api.tawazon.com"
            ]
        },
        {
            "name": "tawazon (Tests)",
            "request": "launch",
            "type": "dart",
            "program": "test/",
            "console": "debugConsole"
        },
        {
            "name": "tawazon (Integration Tests)",
            "request": "launch",
            "type": "dart",
            "program": "integration_test/",
            "args": [
                "--dart-define=ENVIRONMENT=test"
            ]
        },
        {
            "name": "tawazon (Hot Restart)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--hot"
            ],
            "console": "debugConsole"
        },
        {
            "name": "tawazon (Specific Device)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "console": "debugConsole"
        }
    ],
    "inputs": [
        {
            "type": "command",
            "command": "flutter.selectDevice"
        }
    ]
}